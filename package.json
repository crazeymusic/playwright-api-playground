{
  "name": "playwright-api-playground",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "lint-staged": {
    "**/*.{ts,js,cjs,mjs}": [
      "node node_modules/eslint/bin/eslint.js --fix"
    ],
    "**/*.{ts,js,cjs,mjs,json,md,yml,yaml}": [
      "npx --no-install prettier --write"
    ]
  },
  "scripts": {
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "report": "playwright show-report",
    "lint": "node node_modules/eslint/bin/eslint.js .",
    "lint:fix": "node node_modules/eslint/bin/eslint.js . --fix",
    "format": "node node_modules/prettier/bin-prettier.js . --write",
    "format:check": "node node_modules/prettier/bin-prettier.js . --check",
    "typecheck": "node node_modules/typescript/bin/tsc -p tsconfig.json --noEmit",
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "eslint": "^9.39.2",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "zod": "^4.2.1"
  }
}
